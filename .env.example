# Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# Eleven Labs API Configuration (Vite format)
VITE_ELEVEN_LABS_API_KEY=your-elevenlabs-api-key-here
# React format (fallback)
REACT_APP_ELEVEN_LABS_API_KEY=your-elevenlabs-api-key-here

# OpenAI API Configuration (Vite format)
VITE_OPENAI_API_KEY=your-openai-api-key-here
# React format (fallback)
REACT_APP_OPENAI_API_KEY=your-openai-api-key-here

# Note: Please add your OpenAI API key to enable vector embeddings
# Without this key, conversations will be stored without embeddings

# Proxy Configuration (Optional - Use Vercel API routes by default)
# Set VITE_USE_PROXY=true to use separate proxy server instead of Vercel API routes
# VITE_USE_PROXY=false
# VITE_PROXY_URL=http://localhost:3001

# Supabase Configuration (Vite format)
VITE_SUPABASE_URL=https://dhzzbyrmgvmzhwngkrag.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here
# React format (fallback)
REACT_APP_SUPABASE_URL=https://dhzzbyrmgvmzhwngkrag.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key-here